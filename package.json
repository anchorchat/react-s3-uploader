{
  "name": "@anchorchat/react-s3-uploader",
  "version": "1.0.2",
  "description": "React S3 uploader",
  "main": "dist/index.js",
  "files": [
    "/*"
  ],
  "scripts": {
    "watch": "babel -d dist/ -w src/",
    "compile": "rm -rf dist && babel -d dist/ src/",
    "eslint": "eslint '{src,docs/src,test}/**/**/*.{js,jsx}'",
    "copy": "cp -rf package.json README.md dist",
    "build": "npm run compile && npm run copy",
    "dist": "cd dist && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchorchat/react-s3-uploader.git"
  },
  "keywords": [
    "react",
    "s3",
    "uploader"
  ],
  "author": "Sjaak Luthart <sjaak.luthart@anchor.chat>",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anchorchat/react-s3-uploader/issues"
  },
  "homepage": "https://github.com/anchorchat/react-s3-uploader#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "peerDependencies": {
    "prop-types": "^16.3.3",
    "react": "^16.3.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.11"
  }
}
